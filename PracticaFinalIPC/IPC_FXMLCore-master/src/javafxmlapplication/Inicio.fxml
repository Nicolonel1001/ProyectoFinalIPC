<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxmlapplication.InicioController">
    <stylesheets>
        <URL value="@inicio.css" />
    </stylesheets>
    <children>
      <Label layoutX="194.0" layoutY="200.0" text="Pagina de Inicio">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <ButtonBar layoutX="118.0" layoutY="26.0" prefHeight="40.0" prefWidth="322.0">
         <buttons>
            <Button id="AGI" mnemonicParsing="false" onAction="#onAnadirGasto" text="Anadir Gasto" />
            <Button id="VGI" fx:id="botonVerGastos" mnemonicParsing="false" onAction="#onShowExpense" text="Ver mis Gastos" />
         </buttons>
      </ButtonBar>
      <Button layoutX="534.0" layoutY="14.0" mnemonicParsing="false" onAction="#onLogout" text="Logout" />
   </children>
</AnchorPane>
