            </HBox>
         </children>
      </VBox>
      <Button layoutX="507.0" layoutY="340.0" mnemonicParsing="false" onAction="#onCancelar" text="Cancelar" />
      <Button layoutX="434.0" layoutY="341.0" mnemonicParsing="false" onAction="#onCrear" text="Crear" />
